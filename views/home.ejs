<link rel="stylesheet" href="/style.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js" integrity="sha384-+YQ4JLhjyBLPDQt//I+STsc9iw4uQqACwlvpslubQzn4u2UU2UFM80nGisd026JF" crossorigin="anonymous"></script>

<%- include('partials/header'); -%>


<h1>Home</h1>

<p> <%= startingContent %> </p>
<img src= <%= imageURL %> />

<div class="container">
  <div class="row card-deck">
    <div class="col-md-4">
      <div class="card">
        <img class="card-image-top" src="https://thumbs.dreamstime.com/b/channa-masala-indian-dish-prepared-chickpeas-47871577.jpg">
        <div class="card-body">
          <h3 class="card-title"><a href="google.com">Channa Masala</a></h3>
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="card">
        <img class="card-image-top" src="https://media.istockphoto.com/photos/palak-paneer-or-spinach-and-cottage-cheese-curry-on-a-dark-background-picture-id1146291429?s=612x612">
        <div class="card-body">
          <h3 class="card-title"><a href="google.com">Palak paneer</a></h3>
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="card">
        <img class="card-image-top" src="https://media.istockphoto.com/photos/dum-aloo-indian-food-picture-id1256914416">
        <div class="card-body">
          <h3 class="card-title"><a href="google.com">Aloo dum</a></h3>
        </div>
      </div>
    </div>
  </div>
</div>




<% posts.forEach(function(postArr) { %>
  <h1><%=postArr.title%></h1>
  <p>
    <%=postArr.content%>
    <a href="posts/<%=postArr._id%>">Read more</a>
  </p>
<% });%>




<%- include('partials/footer'); -%>
